<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <bootstrap-acme-space-amp j:dependencies="default news"
                                j:modulePriority="0"
                                j:moduleType="module"
                                j:title="bootstrap-acme-space-AMP"
                                jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                jcr:primaryType="jnt:module">
                <sources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-bootstrap-acme-space-amp-1.0-SNAPSHOT">
                    <src jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                        <main jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                            <resources jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                                <jnt_news jcr:mixinTypes="jmix:sourceControl"
                               jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:mixinTypes="jmix:sourceControl"
                              jcr:primaryType="jnt:templateTypeFolder">
                                        <news.detail-amp.jsp jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_news>
                                <jnt_template jcr:mixinTypes="jmix:sourceControl"
                                   jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:mixinTypes="jmix:sourceControl"
                              jcr:primaryType="jnt:templateTypeFolder">
                                        <template.bootstrap-acme-space-amp.jsp jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_template>
                            </resources>
                        </main>
                    </src>
                </sources>
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <news j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="news"/>
                        <amp j:applyOn="jnt:news"
                    j:defaultTemplate="false"
                    j:hiddenTemplate="false"
                    j:view="bootstrap-acme-space-amp"
                    jcr:primaryType="jnt:contentTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="news detail AMP"/>
                        </amp>
                    </news>
                </templates>
                
            </bootstrap-acme-space-amp>
    </modules>
</content>